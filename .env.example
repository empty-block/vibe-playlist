# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Neynar API (for Farcaster data)
NEYNAR_API_KEY=your_neynar_api_key_here

# Neynar Signer UUID (for posting casts to Farcaster)
# This is used to authenticate when publishing casts via the Neynar API
# Get a signer UUID from: https://neynar.com (Dashboard > Signers)
# For MVP testing, you can use your personal Farcaster account's signer
NEYNAR_SIGNER_UUID=your_signer_uuid_here

# Anthropic Claude API (for AI music metadata extraction - TASK-650)
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# AI Worker Configuration (TASK-657)
# Background worker that automatically processes music metadata queue
AI_WORKER_ENABLED=true                # Enable/disable background worker (default: true)
AI_WORKER_INTERVAL_MS=30000           # Processing interval in milliseconds (default: 30000 = 30 seconds)
AI_WORKER_BATCH_SIZE=20               # Maximum tracks to process per batch (default: 20)

# Server Configuration
PORT=4201
NODE_ENV=development
